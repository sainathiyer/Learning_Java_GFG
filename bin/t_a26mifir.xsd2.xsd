<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema version="1.0" elementFormDefault="qualified" targetNamespace="http://www.bafin.de/mvp/a26mifir/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.bafin.de/mvp/a26mifir/">
	<!-- ************************************************************ -->
	<!-- ****************   Include des allgemeinen Schemas   ******* -->
	<!-- ************************************************************ -->
	<xsd:include schemaLocation="t_a26mifir.xsd3.xsd"/>
	<!-- A26MiFIR DATTRA Request -->
	<xsd:complexType name="a26mifirInDATTRAType">
		<xsd:sequence>
			<xsd:element name="dateiname" type="dateinameA26mifirDATTRAType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- A26MiFIR Response -->
	<xsd:complexType name="a26mifirOutType">
		<xsd:sequence>
			<!-- hier ist die MeldungId zu finden -->
			<xsd:element name="meldungsId" type="xsd:int"/>
			<!-- Meldezeitpunkt der Meldung -->
			<xsd:element name="meldezeitpunkt" type="xsd:dateTime"/>
			<!-- Kundenreferenz aus dem Request -->
			<xsd:element name="kundenreferenz" type="kundenreferenzType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="dateinameA26mifirDATTRAType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="A26MiFIR_[a-zA-Z0-9_\.\-]{1,70}\.xml"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="inDATTRA" type="a26mifirInDATTRAType"/>
</xsd:schema>